{
    "compilerOptions": {
      "outDir": "./public/js/",
      "noImplicitAny": true,
      //"module": "es6",
      //"target": "es5",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "lib":[
          "es2015", "dom"
      ]
